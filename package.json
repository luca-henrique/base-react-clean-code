{
  "name": "clean-react",
  "version": "3.1.0",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "author": "Rodrigo Manguinho",
  "homepage": "https://fordevs-react.herokuapp.com",
  "license": "GNU GPL3",
  "scripts": {
    "start": "node index",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "npm run dev:base -- --open",
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:8080 test:cypress:run",
    "check": "npm-check -u -s",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.15.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/axios": "^0.14.0",
    "@types/faker": "^6.6.8",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/experimental-utils": "^5.42.0",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.1.0",
    "favicons-webpack-plugin": "^5.0.2",
    "full-icu": "^1.5.0",
    "git-commit-msg-linter": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.2",
    "jest-localstorage-mock": "^2.4.22",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "sass-loader": "^13.1.0",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "express": "^4.18.2",
    "express-history-api-fallback": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.7.6"
  }
}
